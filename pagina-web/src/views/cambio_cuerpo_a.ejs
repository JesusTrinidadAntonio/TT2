<%- include('partials/_dashboard') %>

    <div class="datos_usuario">
        <form action="/cambio_datos_ca/<%= data.id_cuerpo_a %>" method="post">
            <div class="form-group">
                <input type="text" name="nombre_cuerpo_a" value="<%= data.nombre_cuerpo_a %>" class="form-control">
            </div>
            <div class="form-group">
                <input type="date" name="fecha_cuerpo_a" value="<%= data.fecha_cuerpo_a %>" class="form-control">

            <button type="submit" class="btn btn-outline-success"> GUARDAR</button>
            <a href="/ver_registro/<%= data.id_cuerpo_a %>" class="btn btn-outline-danger"> CANCELAR</a>
        </form>
    </div>